{
    "name": "stubbles/peer",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Help with socket operations.",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-filter": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "mikey179/vfsstream": "^1.6.12",
        "bovigo/assert": "^8.1",
        "bovigo/callmap": "^9.0",
        "stubbles/values": "^12.0"
    },
    "suggest": {
        "stubbles/values": "Will automatically add http uri parsing to stubbles\\values\\Parse"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\peer\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\peer\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "11.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
